<h2>Users List</h2>

<table>
  <thead>
    <tr >
      <th>IDUser</th>
      <th>RoleUser</th>
      <th>Rank</th>
      <th>Name</th>
      <th>EmailUser</th>
      <th>UserPins</th>
      <th>UserCreated_at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= user.id %></td>
      <td><%= user.role %></td>
      <td><%= user.rank %></td>
      <td><%= user.username %></td>
      <td><%= user.email %></td>
      <td><%#= user.pin_ids %></td>
      <td><%= user.created_at %></td>
      <td><%= link_to 'Show', user %></td>
      <td><%= link_to 'Edit', edit_user_registration_path if current_user == user %></td>
      <td>
      </td>

    </tr>
    <% end %>
  </tbody>
</table>
